{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "format": "prettier --write .",
    "format.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\" & stylelint \"src/**/*.css\" & textlint \"{README.md,src/**/*.md}\"",
    "lint.fix": "eslint \"src/**/*.ts*\" --fix & stylelint \"src/**/*.css\" --fix & textlint \"{README.md,src/**/*.md}\" --fix",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.1",
    "@textlint-rule/textlint-rule-no-duplicate-abbr": "^1.0.2",
    "@textlint-rule/textlint-rule-no-unmatched-pair": "^2.0.2",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.6",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-qwik": "^1.4.5",
    "husky": "^9.0.11",
    "prettier": "^3.2.4",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "textlint": "^14.0.3",
    "textlint-rule-alex": "^5.0.0",
    "textlint-rule-alive-link": "^0.1.2",
    "textlint-rule-apostrophe": "^2.0.0",
    "textlint-rule-diacritics": "^1.0.0",
    "textlint-rule-doubled-spaces": "^1.0.2",
    "textlint-rule-en-capitalization": "^2.0.3",
    "textlint-rule-footnote-order": "^1.0.3",
    "textlint-rule-no-zero-width-spaces": "^1.0.1",
    "textlint-rule-preset-ja-spacing": "^2.3.0",
    "textlint-rule-preset-japanese": "^10.0.3",
    "textlint-rule-preset-jtf-style": "^2.3.14",
    "typescript": "5.3.3",
    "undici": "*",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@builder.io/qwik": "^1.4.5",
    "@builder.io/qwik-city": "^1.4.5",
    "@fontsource/fira-code": "^5.0.16",
    "@unocss/reset": "^0.58.5",
    "remixicon": "^4.1.0"
  }
}
